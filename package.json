{
    "name": "carta-frontend",
    "version": "0.1.0",
    "private": true,
    "homepage": "./",
    "dependencies": {
        "@blueprintjs/core": "^2.3.1",
        "golden-layout": "^1.5.9",
        "konva": "^2.1.5",
        "lodash": "^4.17.10",
        "mobx": "^5.0.2",
        "mobx-react": "^5.2.3",
        "plotly.js": "^1.38.3",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-flexbox-grid": "^2.1.2",
        "react-konva": "^1.7.7",
        "react-plotly.js": "^2.2.0",
        "react-resize-detector": "^3.0.1",
        "react-scripts-ts": "^2.16.0",
        "react-transition-group": "^2.3.1",
        "rxjs": "^6.2.1",
        "source-map-explorer": "^1.5.0"
    },
    "scripts": {
        "analyze": "source-map-explorer build/static/js/main.*",
        "build-css": "node-sass-chokidar -q src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar -q src/ -o src/ --watch --recursive",
        "build-wrappers": "./wasm_src/build_wrappers.sh",
        "watch-wrappers": "./wasm_src/build_wrappers.sh --watch",
        "build-protobuf": "./protobuf/build_proto.sh",
        "watch-protobuf": "npx chokidar --initial -d 4000 './protobuf/**/*.proto' -c './protobuf/build_proto.sh'",
        "start-js": "react-scripts-ts start",
        "start": "npm-run-all -p watch-css start-js watch-wrappers watch-protobuf",
        "build-js": "react-scripts-ts build",
        "build": "npm-run-all build-wrappers build-protobuf build-css build-js",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject"
    },
    "devDependencies": {
        "@types/jest": "^22.2.3",
        "@types/jquery": "^3.3.2",
        "@types/lodash": "^4.14.109",
        "@types/node": "^9.6.21",
        "@types/plotly.js": "^1.38.1",
        "@types/react": "^16.3.17",
        "@types/react-dom": "^16.0.6",
        "@types/react-resize-detector": "^2.2.0",
        "chokidar-cli": "^1.2.0",
        "mobx-react-devtools": "^5.0.1",
        "node-sass-chokidar": "^1.3.0",
        "npm-run-all": "^4.1.3",
        "protobufjs": "^6.8.6",
        "typescript": "^2.9.2"
    }
}
